{
  "name": "d3-projects",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8020",
    "build": "next build",
    "start": "next start -p 8020",
    "prettier": "prettier --write .",
    "prettier:fix": "prettier --write .",
    "eslint": "eslint . --ext js --ext jsx",
    "eslint:fix": "eslint . --ext js --ext jsx --fix",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint --fix '**/*.scss'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run eslint && npm run stylelint && npm run prettier"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "d3": "^6.2.0",
    "next": "^10.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^11.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "stylelint": "^13.7.2",
    "stylelint-scss": "^3.18.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
