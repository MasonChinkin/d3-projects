{
  "name": "d3-projects",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8020",
    "build": "next build",
    "start": "next start -p 8020",
    "format": "prettier --write .",
    "lint": "eslint . --ext js --ext jsx",
    "lint:fix": "eslint . --ext js --ext jsx --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(js|jsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "d3": "^6.2.0",
    "next": "^10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@testing-library/react": "^11.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "jest-watch-typeahead": "^0.6.1",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "sass": "^1.28.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
