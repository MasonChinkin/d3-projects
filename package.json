{
  "name": "d3-projects",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit:skip": "HUSKY_SKIP_HOOKS=1 git commit -m '$1'",
    "dev": "next dev -p 8020",
    "eslint:fix": "eslint . --ext js --ext jsx --fix",
    "eslint": "eslint . --ext js --ext jsx",
    "format:fix": "prettier --write .",
    "format:pre-commit": "pretty-quick --staged",
    "postinstall": "husky install",
    "push:skip:f": "HUSKY_SKIP_HOOKS=1 git push -f",
    "push:skip": "HUSKY_SKIP_HOOKS=1 git push",
    "start": "next start -p 8020",
    "stylelint:fix": "stylelint --fix '**/*.scss'",
    "stylelint": "stylelint '**/*.scss'",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,yml,yaml,css,scss,md}": [
      "npm run format:pre-commit",
      "npm run eslint",
      "npm run stylelint"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "d3": "^6.2.0",
    "next": "^10.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "sass": "^1.29.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-use-nesting": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
